<?xml version="1.0"?>

<PropertyList>

 <path>tu154.ac</path>

 <offsets>
   <z-m>-0.5</z-m>
   <x-m>-23.00</x-m>
 </offsets> 

 <!-- Object Selection -->

 <animation>
  <type>select</type>
  <object-name>leftgear</object-name>
  <object-name>rightgear</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[1]/position-norm</property>
    <value>0.01</value>
   </greater-than>
  </condition>
 </animation>

 <!-- Object Movement -->

 <animation>
  <type>rotate</type>
  <object-name>leftgear</object-name>
  <property>/gear/gear[1]/position-norm</property>
  <factor>90</factor>
  <offset-deg>-90</offset-deg>
  <center>
   <x-m>25.9</x-m>
   <y-m>-5.65</y-m>
   <z-m>-1.5</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>rightgear</object-name>
  <property>/gear/gear[1]/position-norm</property>
